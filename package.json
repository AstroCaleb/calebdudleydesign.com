{
  "name": "calebdudleydesign",
  "version": "1.0.0",
  "description": "Caleb Dudley Design",
  "author": "Caleb Dudley",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --no-error-on-unmatched-pattern --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "prepare": "husky",
    "checks-before-push": "yarn lint && yarn format:check && yarn type-check"
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-intersection-observer": "^9.16.0",
    "sweetalert2": "^11.23.0",
    "sweetalert2-react-content": "^5.1.0",
    "yet-another-react-lightbox": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/fslightbox-react": "^1.8.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "prettier": "3.6.2",
    "process": "^0.11.10",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "packageManager": "yarn@4.10.2"
}
